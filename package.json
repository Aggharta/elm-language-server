    "execa": "^2.0.4",
    "fast-diff": "^1.2.0",
    "globby": "^10.0.1",
    "request": "^2.88.0",
    "rxjs": "^6.5.2",
    "vscode-languageserver": "^5.2.1",
    "vscode-uri": "^2.0.3",
    "web-tree-sitter": "^0.15.9",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.18",
    "@types/node": "12.7.3",
    "@types/request": "^2.48.2",
    "@types/ws": "^6.0.3",
    "clean-webpack-plugin": "^3.0.0",
