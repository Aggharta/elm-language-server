{
  "name": "@elm-tooling/elm-language-server",
  "description": "Implementation of an elm language server in node.",
  "version": "1.4.1",
  "author": "Kolja Lampe",
  "license": "MIT",
  "files": [
    "out/tree-sitter-elm.wasm",
    "out/tree-sitter.wasm",
    "out/index.js"
  ],
  "engines": {
    "node": "*"
  },
  "repository": {
