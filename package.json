    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "shebang-loader": "0.0.1",
    "source-map-support": "^0.5.12",
    "tree-sitter-cli": "^0.15.8",
    "tree-sitter-elm": "^2.7.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.0.3",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "3.6.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "scripts": {
    "copy-wasm": "copyfiles ./tree-sitter-elm.wasm out",
    "compile": "webpack -p",
    "watch": "webpack -w -d",
    "lint": "tslint -p tsconfig.json",
    "format:fix": "pretty-quick --staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "build-tree-sitter": "tree-sitter build-wasm ./node_modules/tree-sitter-elm && npm run copy-wasm",
    "test": "jest"
  },
  "bin": {
    "elm-language-server": "./out/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "elm",
    "elm-lang",
    "language server",
    "language-server",
    "lsp"
  ]
}
